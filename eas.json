{
    "cli": {
        "version": ">= 16.26.0",
        "appVersionSource": "remote"
    },
    "build": {
        "development": {
            "developmentClient": true,
            "distribution": "internal",
            "channel": "development",
            "node": "22.15.0",
            "env": {
                "NODE_VERSION": "22.15.0"
            }
        },
        "preview": {
            "distribution": "internal",
            "channel": "preview",
            "android": {
                "buildType": "apk"
            },
            "node": "22.15.0"
        },
        "production": {
            "channel": "production",
            "autoIncrement": true,
            "android": {
                "buildType": "apk"
            },
            "node": "22.15.0"
        }
    },
    "submit": {
        "production": {}
    }
}
